{
  "name": "zuju-theme",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sass:global": "sass --watch --update ./zuju-theme/scss:./zuju-theme/css",
    "sass:module": "sass --watch --update ./zuju-theme/modules:./zuju-theme/modules",
    "watch:dev": "hs watch --account=zuju-staging zuju-theme zuju-theme --remove",
    "watch:prod": "hs watch --account=dallah zuju-theme zuju-theme --remove --initial-upload",
    "watch": "hs watch zuju-theme zuju-theme --remove",
    "fetch": "hs fetch zuju-theme zuju-theme --overwrite"
  },
  "devDependencies": {
    "@hubspot/cli": "^3.0.9",
    "sass": "^1.39.2"
  }
}